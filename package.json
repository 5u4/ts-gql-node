{
  "name": "ts-gql-neo-node",
  "version": "1.0.0",
  "description": "ðŸ¥³ Typescript + GraphQL + Neo4J + Node.js template",
  "main": "index.js",
  "scripts": {
    "generate": "yarn merge-schemas && yarn gql2ts && yarn graphdoc",
    "gql2ts": "yarn merge-schemas && ts-node ./src/scripts/gql2ts.ts",
    "graphdoc": "yarn merge-schemas && graphdoc -s ./src/schemas/schema.merged.graphql -o ./docs/graphql -f",
    "merge-schemas": "ts-node ./src/scripts/mergeSchemas.ts",
    "serve": "nodemon --exec ts-node --files src/server.ts",
    "test": "mocha --require ts-node/register ./tests/**/*.test.ts --files --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/senhungwong/ts-gql-neo-node.git"
  },
  "keywords": [
    "typescript",
    "graphql",
    "neo4j",
    "nodejs"
  ],
  "author": "Senhung Wong <0x53656e@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/senhungwong/ts-gql-neo-node/issues"
  },
  "homepage": "https://github.com/senhungwong/ts-gql-neo-node#readme",
  "dependencies": {
    "@senhung/http-exceptions": "^1.1.1",
    "apollo-server": "^2.3.1",
    "dotenv": "^6.2.0",
    "graphql-import": "^0.7.1",
    "graphql-yoga": "^1.16.9",
    "merge-graphql-schemas": "^1.5.8",
    "simple-neo4j": "^4.3.1",
    "yup": "^0.26.6"
  },
  "devDependencies": {
    "@2fd/graphdoc": "^2.4.0",
    "@gql2ts/from-schema": "^1.10.1",
    "@types/chai": "^4.1.7",
    "@types/chai-http": "^3.0.5",
    "@types/dotenv": "^6.1.0",
    "@types/expect": "^1.20.3",
    "@types/mocha": "^5.2.5",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.12.12",
    "@types/yup": "^0.26.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "mocha": "^5.2.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.8",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.2"
  }
}